prefix=@prefix@
exec_prefix=@exec_prefix@
libdir=@libdir@
includedir=@includedir@

Name: MarTo
Description: Markov Toolkit for Markov models simulation library
Version: @VERSION@
URL: @PACKAGE_URL@
Requires: @MARTOLIB_PKGCONFIG_REQUIRES@
